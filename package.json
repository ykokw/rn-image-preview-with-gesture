{
  "dependencies": {
    "@types/jest": "^29.5.8",
    "expo": "^49.0.0",
    "expo-dev-client": "~2.4.12",
    "expo-image": "~1.3.1",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "jest": "^29.2.1",
    "jest-expo": "~49.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.1",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@config-plugins/detox": "^6.0.0",
    "@types/node": "^20.9.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "~0.70.6",
    "detox": "^20.13.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.3"
  },
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:ios": "detox build -c ios.sim.release",
    "test:ios": "detox test -c ios.sim.release",
    "e2e:ios": "npm run build:ios && npm run test:ios",
    "build:android": "detox build -c android.emu.debug",
    "test:android": "detox test -c android.emu.debug",
    "e2e:android": "npm run build:android && npm run test:android",
    "clean:android": "pushd android && ./gradlew clean && popd"
  },
  "version": "1.0.0",
  "private": true,
  "name": "image-preview-with-gesture",
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ]
  }
}
